
<div class="row">
  <table class="table table-striped table-bordered table-hover table-condensed index-table" style="width:100%;">
    <thead>
      <tr>
            <th class="id-col"><%= sortable "id", "Id" %></th>
            <th><%= sortable "subject", "Subject" %></th>
            <th><%= sortable "message", "Message" %></th>
            <th><%= sortable "created_at", "created_at" %></th>
                                  
      </tr>
    </thead>

    <tbody>
      <%= content_tag_for(:tr, @dev_notes) do |item| %>
            <td><%= item.id %></td>
            <td><%= item.subject %></td>
            <td><%= item.message %></td>
            <td><%= pretty_datetime item.created_at %></td>
            
              
      <% end %>
    </tbody>
  </table>
</div>

<%= paginate @dev_notes %>